package com.autowire.annotation;

import org.springframework.beans.factory.annotation.Autowired;

public class Mobile {

	
	@Autowired
	private Apps app;

	public Mobile() {
		System.out.println("no-arg constructor of Mobile");
	}
	public Mobile(Apps app) {
		this.app = app;
		System.out.println("arg-cons of Mobile");
	}

	@Override
	public String toString() {
		return "Mobile [app=" + app + "]";
	}

	public Apps getApp() {
		return app;
	}

	public void setApp(Apps app) {
		this.app = app;
		System.out.println("setters of Mobile");
	}
	
	
	
	
	
}
